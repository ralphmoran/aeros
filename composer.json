{
    "name": "aero-admin/v1.0",
    "type": "project",
    "autoload":{
        "files":[
            "lib/functions/all.php",
            "config/session.php"
        ],
        "classmap": [
            "lib/classes",
            "lib/interfaces",
            "lib/traits",
            "controllers",
            "models",
            "middlewares",
            "providers",
            "events"
        ]
    },
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "aws/aws-sdk-php": "^3.211",
        "firebase/php-jwt": "^6.1",
        "sendgrid/sendgrid": "~7",
        "twilio/sdk": "^6.36",
        "composer/ca-bundle": "^1.3",
        "predis/predis": "^2.0",
        "sentry/sdk": "^3.2",
        "robmorgan/phinx": "^0.13.4",
        "fzaninotto/faker": "^1.5",
        "pda/pheanstalk": "^4.0"
    },
    "require-dev": {
        "robmorgan/phinx": "^0.13.4"
    },
    "license": "Property",
    "minimum-stability": "alpha",
    "scripts": {
        "uitest": "php ./run-tests.php",
        "uimaker": "php ./vendor/rafael.moran/uitest/uimaker"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
