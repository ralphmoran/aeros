{
    "name": "aero-admin/v1.0",
    "type": "project",
    "autoload":{
        "files":[
            "lib/functions/all.php",
            "config/session.php"
        ],
        "psr-4": {
            "Classes\\": "lib/classes",
            "Interfaces\\": "lib/interfaces",
            "Traits\\": "lib/traits",
            "Controllers\\": "controllers",
            "Models\\": "models",
            "Middlewares\\": "middlewares",
            "Providers\\": "providers",
            "Events\\": "events",
            "Crons\\": "queues/crons",
            "Jobs\\": "queues/jobs",
            "Workers\\": "queues/workers"
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "aws/aws-sdk-php": "^3.211",
        "firebase/php-jwt": "^6.1",
        "sendgrid/sendgrid": "~7",
        "twilio/sdk": "^6.36",
        "composer/ca-bundle": "^1.3",
        "predis/predis": "^2.0",
        "sentry/sdk": "^3.2",
        "robmorgan/phinx": "^0.13.4",
        "fzaninotto/faker": "^1.5",
        "pda/pheanstalk": "^4.0"
    },
    "require-dev": {
        "robmorgan/phinx": "^0.13.4"
    },
    "license": "Property",
    "minimum-stability": "alpha",
    "scripts": {
        "app-worker-start": "sudo supervisorctl start app-worker-script:*",
        "app-worker-stop": "sudo supervisorctl stop app-worker-script:*",
        "app-worker-status": "sudo supervisorctl status app-worker-script:*",
        "app-worker-start-dev": "vagrant ssh -c 'sudo supervisorctl start app-worker-script:*'",
        "app-worker-stop-dev": "vagrant ssh -c 'sudo supervisorctl stop app-worker-script:*'",
        "app-worker-status-dev": "vagrant ssh -c 'sudo supervisorctl status app-worker-script:*'"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
