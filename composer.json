{
    "name": "aeros/framework",
    "description": "The most lightweight MVC framework for PHP development.",
    "type": "project",
    "autoload":{
        "files":[
            "src/Helpers/all.php"
        ],
        "psr-4": {
            "Aeros\\Src\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "vlucas/phpdotenv": "^5.5",
        "firebase/php-jwt": "^6.3",
        "sendgrid/sendgrid": "~8.1",
        "twilio/sdk": "^6.43",
        "composer/ca-bundle": "^1.3",
        "predis/predis": "^2.1",
        "sentry/sdk": "^4.0",
        "robmorgan/phinx": "^0.13.4",
        "fakerphp/faker": "^1.22",
        "pda/pheanstalk": "^4.0",
        "ramsey/uuid": "^4.7",
        "symfony/process": "^6.0.19",
        "phpseclib/phpseclib": "^3.0",
        "symfony/cache": "^6.0.19",
        "symfony/console": "^6.0.19",
        "peppeocchi/php-cron-scheduler": "^4.0",
        "guzzlehttp/guzzle": "^7.8",
        "phpmailer/phpmailer": "^6.9",
        "openswoole/core": "^22.1"
    },
    "require-dev": {
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "scripts": {
        "post-create-project-cmd": []
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
